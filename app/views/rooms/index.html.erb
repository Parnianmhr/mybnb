
<div class="col-md-9 rooms">
    <div class="panel panel-primary">
        <div class="panel-heading">
            Your Listings
        </div>
        <div class="panel-body">
            <% @rooms.each do |room| %>
            <div class="row">
                <div class="col-md-7">
                    <h4><%= room.listing_name %></h4>
                </div>
                <div class="col-md-3 right">
                    <%= link_to "View", room, class: "btn btn-default" %>
                    <%= link_to "Update", edit_room_path(room), class: "btn btn-default" %>
                </div>
            </div>
            <% end %>

        </div>
    </div>

    <%= link_to "Add new place", new_room_path, class: "btn btn-yellow" %>
</div>
